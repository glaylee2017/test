<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huaiwang.starsky.mapper.manage.ActiveCodeMapper">
	<select id="queryActiveCode" resultType="ActiveCode" parameterType="String">
		select * from tb_active_code where code = #{activeCode} and phone_num = #{phoneNum};
	</select>
</mapper>